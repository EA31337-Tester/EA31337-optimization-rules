## Init settings.
# Parameter list to optimize.
param=TrendMethod
param_list=( $param )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test $param.
input_set $param,F 1
input_set $param,1 0
input_set $param,2 7
input_set $param,3 255

