## Init settings.
# Parameter list to optimize.
param_list=( Account_Conditions_Active )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Account_Conditions_Active.
input_set Account_Conditions_Active,F 1
input_set Account_Conditions_Active,1 0
input_set Account_Conditions_Active,2 1
input_set Account_Conditions_Active,3 1

